   "source": [
    "# reverse strings without reversing words, separated by .\n",
    "\n",
    "def reversewords(s):\n",
    "    words = s.split(\".\")\n",
    "    words.reverse()\n",
    "    x = \".\".join(words)\n",
    "    print(x)\n",
    "    \n",
    "s = \"i.like.this.program.very.much\"\n",
    "reversewords(s)"
   ]
  }
